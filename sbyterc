setcmd TOGGLE_CONVERTER             EQUALS
setcmd CURSOR_DOWN                  J_LOWER
setcmd CURSOR_UP                    K_LOWER
setcmd CURSOR_LEFT                  H_LOWER
setcmd CURSOR_RIGHT                 L_LOWER
setcmd CURSOR_LENGTH_DOWN           J_UPPER
setcmd CURSOR_LENGTH_UP             K_UPPER
setcmd CURSOR_LENGTH_LEFT           H_UPPER
setcmd CURSOR_LENGTH_RIGHT          L_UPPER

setcmd JUMP_TO_REGISTER             G_UPPER
setcmd DELETE                       X_LOWER
setcmd YANK                         Y_LOWER
setcmd PASTE                        P_LOWER
setcmd UNDO                         U_LOWER
setcmd REDO                         CTRL+R
setcmd CLEAR_REGISTER               ESCAPE,ESCAPE
setcmd INCREMENT                    PLUS
setcmd DECREMENT                    DASH
setcmd BACKSPACE                    BACKSPACE
setcmd DELETE                       DELETE

setcmd MODE_SET_INSERT              I_LOWER
setcmd MODE_SET_INSERT_SPECIAL      I_UPPER
setcmd MODE_SET_OVERWRITE           O_LOWER
setcmd MODE_SET_OVERWRITE_SPECIAL   O_UPPER
setcmd MODE_SET_APPEND              A_LOWER
setcmd MODE_SET_SEARCH              SLASH
setcmd MODE_SET_CMD                 COLON

